            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="159*"/>
                <ColumnDefinition Width="97*"/>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition/>
            </Grid.RowDefinitions>

            <RadioButton Content="Analysis Settings"
                         IsChecked="True"
                         IsEnabled="False"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Center"
                         Height="50"
                         Foreground="White"
                         Style="{StaticResource MenuButtonTheme}"
                         FontSize="12" Width="90" Margin="10,0,0,0"/>
            <RadioButton Content="Analysis Results"
                         IsEnabled="False"
             HorizontalAlignment="Left"
             VerticalAlignment="Center"
             Height="50"
             Foreground="White"
                         Style="{StaticResource MenuButtonTheme}"
             FontSize="12" Margin="105,0,0,0" Width="90"/>
