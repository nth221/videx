                Foreground="White"
                FontSize="16"
                TextDecorations="Underline"
                HorizontalAlignment ="Left"
                Margin="19,19,0,434" RenderTransformOrigin="0.568,1.028" FontWeight="Bold" Width="44"/>
                    </Border>
                    <CheckBox Visibility="{Binding CheckBoxVisibility}" Content="{Binding Videofilename}"  Margin="26,85,33,263" Foreground="#FFFBFBFB"/>
                </Grid>
                <Grid Grid.ColumnSpan="2">
                    <Border Background="#323345"
CornerRadius="10,10,0,0" Margin="229,0,266,175"/>
                </Grid>
                <Grid Grid.ColumnSpan="2">
                    <Border Background="#323345" CornerRadius="10,10,0,0" Margin="225,0,278,175">
                        <Grid>
                            <ContentControl Content="{Binding VideoObject}"  Margin="10,10,10,65"/>
                            <Slider x:Name="sldrPlayTime" Margin="10,0,10,42" VerticalAlignment="Bottom" 
                  Value="{Binding SlderPlayTime}"
                  Minimum="{Binding SliderMinimum}"
                  Maximum="{Binding SliderMaximum}"
                  Thumb.DragStarted="SldrPlayTime_DragStarted"
                  Thumb.DragCompleted="SldrPlayTime_DragCompleted"
                  ValueChanged="SldrPlayTime_ValueChanged"/>
                            <Button Command="{Binding PlayCommand}" Content="▶" HorizontalAlignment="Left" Margin="285,0,0,12" Width="29" Height="23" VerticalAlignment="Bottom"  Background="#FFFFF8F8"/>
                            <Button Command="{Binding StopCommand}" Content="■" HorizontalAlignment="Left" Margin="316,0,0,12" Width="29" Height="23" VerticalAlignment="Bottom"  Background="#FFFFF8F8"/>
                            <Button Command="{Binding PauseCommand}" Content="II" HorizontalAlignment="Left" Margin="348,0,0,12" Width="29" Height="23" VerticalAlignment="Bottom"  Background="#FFFFF8F8"/>
                            <Label Content="Volume" HorizontalAlignment="Left" Margin="503,449,0,0" Height="26" VerticalAlignment="Top" Foreground="White" FontWeight="Bold"/>
                            <Slider x:Name="sldrVolume" HorizontalAlignment="Left" Margin="563,0,0,14" Width="100" Height="18" VerticalAlignment="Bottom" 
Maximum="1" LargeChange="0.1" Value="0.5" />
                            <Label x:Name="lblPlayTime" Content="{Binding LblPlayTime}" HorizontalAlignment="Left" Margin="68,0,0,11" VerticalAlignment="Bottom" Width="212" Height="23" Foreground="White" FontWeight="Bold"/>
